<%= button_to 'Discover Top Rated Movies', "/users/#{params[:user_id]}/movies", method: :get %>

<%= form_with url: "/users/#{params[:user_id]}/movies" , method: :get, local: true do |f| %>

  <%= f.label :Movie_Title %>
  <%= f.text_field :Movie_Title %>

  <%= f.submit 'Search by Movie Title' %>
<% end %>