<div id="user-buttons">
  <%= button_to "Sign In", login_path, method: :get %>
  <%= button_to "Create an Account", register_path, method: :get %>
</div>

<div id="all-users">
  <h4>Existing Users</h4>
  <% @users.each do |user| %>
    <p><%= link_to user.email, user_path(user)%></p>
  <% end %>
</div>