<%= render partial: "shared/nav" %>

<h1>Viewing Party</h1>

<% if current_user %>
  <%= button_to 'Log Out', root_path, method: :delete %><br>
  <h2>Existing Users</h2>
  <% @users.each do |user| %>
    <ul>
      <li><%= link_to user.email, user_path(user.id) %></li>
    </ul>
  <% end %>
<% else %>
  <%= button_to 'Create New User', register_user_path, method: :get %><br>
  <%= button_to 'Log In', login_path, method: :get %><br>
<% end %>





