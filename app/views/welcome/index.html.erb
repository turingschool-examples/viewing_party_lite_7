<%= button_to "Create a New User", register_path, method: :get %>

<h3> Existing User </h3>

<% if current_user %>
  <%= link_to "Log Out", logout_path, method: :post %></br>
<% else %>
  <%= link_to "Log In", login_path %></br>
<% end %>

<% if current_user %>
  <% @users.each do |user| %>
    <%= user.email %></br>
  <% end %>
<% end %>