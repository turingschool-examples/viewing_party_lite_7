<h1>Movie Results</h1>

<table id="movie-results">
  <thead>
    <tr>
      <th>Title</th>
      <th>Vote Average</th>
    </tr>
  </thead>
  <tbody>
    <% @movies.each do |movie| %>
      <tr>
        <td><%= link_to movie.title, user_movie_path(id: movie.id) %></td>
        <td><%= movie.vote_average %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'Return to Discover Page', user_discover_index_path(user_id: @user.id) %>
