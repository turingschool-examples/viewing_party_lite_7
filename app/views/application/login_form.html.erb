<ul>
  <li><a class="active" href="/">Home</a></li>
</ul>


<div class="content">
  <h2>Log In</h2>
<%= form_with url: login_path, method: :post do |form| %>
  <%= form.label :email, "Email:" %>
  <%= form.text_field :email %>
  <%= form.label :password, "Password:" %>
  <%= form.password_field :password %>
  <%= form.submit "Log In" %>
<% end %>
</div>