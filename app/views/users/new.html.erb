<%= render partial: "app_partials/home_link" %>
<%= render partial: "app_partials/viewing_header" %>
<h6> Register a New User </h6>

<%=form_with url: '/register', method: :post, data: { turbo: false } do |form| %>
  <%= form.label :name, "Name:" %>
  <%= form.text_field :name %><br>
  <%= form.label :email, "Email" %>
  <%= form.text_field :email %><br>
  <%= form.submit "Register" %>
<% end %>
