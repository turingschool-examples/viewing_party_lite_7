<%= render partial: "app_partials/home_link" %>
<%= render partial: "app_partials/viewing_header" %>
<%= button_to 'Find Top Rated Movies', "/users/#{@user.id}/movies?q=top%20rated", params: { query: 'top_rated' }, method: :get  %>
<%= form_with url: "/users/#{@user.id}/movies?q=keyword" , method: :get, data: { turbo: false } do |form|%>
<%= form.label :query %>
<%= form.text_field :query %>
<%= form.submit "Find Movies" %>
<% end %>
